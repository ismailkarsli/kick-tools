{
	"manifest_version": 3,
	"name": "Kick Tools",
	"version": "2.1.1",
	"description": "Enhance Kick.com",
	"icons": {
		"48": "icons/48.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.kick.com/*"
			],
			"js": [
				"index.js"
			],
			"run_at": "document_idle"
		}
	],
	"host_permissions": [
		"*://kick.com/*"
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "{e2a9c744-9b82-418f-bced-5c0d83316ac0}",
			"strict_min_version": "116.0"
		}
	}
}